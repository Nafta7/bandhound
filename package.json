{
  "name": "bandhound",
  "description": "Music discovery webapp",
  "author": "Nefla",
  "private": true,
  "scripts": {
    "clean": "rimraf dist/*.map",
    "development": "webpack --mode development",
    "production": "webpack --mode production",
    "stats": "webpack -p --json | webpack-bundle-size-analyzer",
    "analysis": "webpack --mode production --env.platform=analysis",
    "start": "webpack-dev-server --mode development --inline --debug --output-pathinfo --watch",
    "build": "webpack-dev-server",
    "site:push": "gh-pages -d dist",
    "deploy": "npm run production && npm run clean && npm run site:push"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "lodash.get": "^4.4.2",
    "prop-types": "^15.6.1",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-feather": "^1.1.0",
    "react-router-dom": "^4.2.2",
    "slugg": "^1.2.1",
    "youtube-player": "^5.5.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "browser-sync": "^2.24.4",
    "browser-sync-webpack-plugin": "^2.2.2",
    "css-loader": "^0.28.11",
    "gh-pages": "^1.2.0",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.4.0",
    "node-sass": "^4.9.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.0.2",
    "style-loader": "^0.21.0",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "webpack": "^4.10.2",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-bundle-size-analyzer": "^2.2.0",
    "webpack-cli": "^3.0.1",
    "webpack-dev-server": "^3.1.4"
  }
}
