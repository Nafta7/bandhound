{
  "name": "bandhound",
  "description": "Music discovery webapp",
  "author": "Nafta",
  "private": true,
  "scripts": {
    "clean": "rimraf dist/*.map",
    "development": "webpack --env.development",
    "production": "webpack --env.production",
    "stats": "webpack -p --json | webpack-bundle-size-analyzer",
    "analyze": "webpack --env.production --env.platform=analyze",
    "start": "webpack-dev-server --inline -d --watch",
    "build": "webpack-dev-server",
    "site:push": "gh-pages -d dist",
    "deploy": "npm run production && npm run clean && npm run site:push"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-feather": "^1.0.7",
    "react-router-dom": "^4.2.2",
    "youtube-player": "^5.4.0"
  },
  "devDependencies": {
    "babel-core": "^6.17.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "browser-sync": "^2.18.13",
    "browser-sync-webpack-plugin": "^1.1.3",
    "css-loader": "^0.28.7",
    "extract-text-webpack-plugin": "^3.0.1",
    "gh-pages": "^1.0.0",
    "html-webpack-plugin": "^2.22.0",
    "json-loader": "^0.5.4",
    "node-sass": "^4.5.3",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "uglifyjs-webpack-plugin": "^1.0.0-beta.3",
    "webpack": "^3.6.0",
    "webpack-bundle-analyzer": "^2.9.0",
    "webpack-bundle-size-analyzer": "^2.2.0",
    "webpack-dev-server": "^2.9.1"
  }
}
