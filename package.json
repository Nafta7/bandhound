{
  "name": "bandhound",
  "description": "Music discovery webapp",
  "author": "Nefla",
  "private": true,
  "scripts": {
    "clean": "rimraf dist/*.map",
    "development": "webpack --env.development",
    "production": "webpack --env.production",
    "stats": "webpack -p --json | webpack-bundle-size-analyzer",
    "analyze": "webpack --env.production --env.platform=analyze",
    "start": "webpack-dev-server --inline --debug --output-pathinfo --watch",
    "build": "webpack-dev-server",
    "site:push": "gh-pages -d dist",
    "deploy": "npm run production && npm run clean && npm run site:push"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "lodash.get": "^4.4.2",
    "prop-types": "^15.6.1",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-feather": "^1.1.0",
    "react-router-dom": "^4.2.2",
    "slugg": "^1.2.1",
    "youtube-player": "^5.5.0"
  },
  "devDependencies": {
    "babel-core": "^6.17.0",
    "babel-loader": "^7.1.4",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "browser-sync": "^2.23.6",
    "browser-sync-webpack-plugin": "^1.1.3",
    "css-loader": "^0.28.11",
    "extract-text-webpack-plugin": "^3.0.2",
    "gh-pages": "^1.1.0",
    "html-webpack-plugin": "^2.22.0",
    "json-loader": "^0.5.4",
    "node-sass": "^4.8.3",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.7",
    "style-loader": "^0.19.1",
    "uglifyjs-webpack-plugin": "^1.2.4",
    "webpack": "^3.11.0",
    "webpack-bundle-analyzer": "^2.11.1",
    "webpack-bundle-size-analyzer": "^2.2.0",
    "webpack-dev-server": "^2.11.2"
  }
}
